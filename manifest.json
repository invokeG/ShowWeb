{
    "manifest_version": 3,
    "name": "ShowWeb",
    "version": "0.1.0",
    "icons": {
        "48": "images/showwebicon.png"
    },
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_icon": {
            "48": "images/showwebicon.png"
        }
    },
    "permissions": ["scripting", "activeTab", "storage"],
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"],
            "css": ["content.css"]
        }
    ]
}
